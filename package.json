{
  "name": "positional",
  "version": "1.0.0",
  "description": "Making it easy to work with positional flat files.",
  "main": "src/main.js",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "nedb": "^1.8.0",
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^4.0.8",
    "electron-packager": "^13.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "jasmine": "^3.3.1",
    "less": "^3.9.0"
  },
  "scripts": {
    "less": "lessc src/assets/css/main.less src/assets/css/main.css",
    "start": "npm run less && electron .",
    "test": "node jasmine_runner.js",
    "package-windows": "npm run less && electron-packager . --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=release-builds",
    "package-linux": "npm run less && electron-packager . --overwrite --asar --platform=linux --arch=x64 --prune=true --out=release-builds",
    "package": "npm run package-windows && npm run package-linux",
    "lint": "eslint src --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedroppinheiro/positional.git"
  },
  "keywords": [
    "positional",
    "flat",
    "files",
    "tool"
  ],
  "author": "Pedro Pinheiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pedroppinheiro/positional/issues"
  },
  "homepage": "https://github.com/pedroppinheiro/positional#readme"
}
